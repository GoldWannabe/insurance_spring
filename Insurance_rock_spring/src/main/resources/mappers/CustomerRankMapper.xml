<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="CustomerRankMapper">
	
	<select id="selectRenewCustomerRank" resultType="com.mju.spring.dto.contractTeam.contractManagement.RenewCustomerRankDto">
		SELECT * from
		customerRank where contractID = #{contractID}
	</select>
	
	<select id="selectRankID" resultType="string">
		SELECT RankID from customerRank where contractID = #{contractID}
	</select>
	
	<insert id="create">
		INSERT INTO customerRank(customerID, contractID, RankID) VALUES (#{customerID}, #{contractID}, #{rankID})
	</insert>
	
	<delete id="deleteCustomerRank">
		DELETE FROM customerRank WHERE RankID = #{rankID}
	</delete>
</mapper>